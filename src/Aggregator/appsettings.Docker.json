{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=aggregator;Username=postgres;Password=postgres;"
  },
  "Database": {
    "ConnectionString": "Host=postgres;Port=5432;Database=aggregator;Username=postgres;Password=postgres;",
    "CommandTimeout": 30,
    "MaxRetryAttempts": 3,
    "EnableSqlLogging": false
  },
  "HttpClient": {
    "TimeoutSeconds": 30,
    "HandlerLifetimeMinutes": 5,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000,
    "IgnoreSslErrors": true,
    "UserAgent": "Aggregator Bot 1.0 (Docker)"
  },
  "Parsing": {
    "DelayBetweenRequestsMs": 1000,
    "ImagesFolder": "Images",
    "SaveImages": true,
    "EnableDetailedLogging": false,
    "EnabledParsers": [ "AskStudio", "ZNWR" ]
  }
} 